    	<ChoiceDataElement Id="ObservationCharacter" IsRequired="true" DisplaySequence="1" Editable="true" Hasprefilled="false" Output="true">        	
    		<CodableConcept Name="ObservationCharacter">
    			<Coding>
    				<System Value="RadElement"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDE65"></Code>
    				<Display Value="RDE65"></Display>
