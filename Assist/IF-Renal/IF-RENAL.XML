 <DataElements>
 
 
 
    <ChoiceDataElement Id="catfat" DisplaySequence="1">
		<Label> Does the incidental renal mass contain fat? </Label>
			<ChoiceInfo>
				<Choice>
					<Value>yes</Value>
					<Label>Yes, it contains fat </Label>
				</Choice>
				<Choice>
					<Value>no</Value>
					<Label>No, it does not contain fat</Label>
				</Choice>
			</ChoiceInfo>
	</ChoiceDataElement>
 

 
	<ChoiceDataElement Id="catrenalmass" DisplaySequence="2">
		<Label> What is the category of renal mass? </Label>
			<ChoiceInfo>
				<Choice>
					<Value>Thomo</Value>
					<Label>TSTC homogeneous</Label>
				</Choice>
				<Choice>
					<Value>homo</Value>
					<Label>Homogeneous on unenhanced CT</Label>
				</Choice>
				<Choice>
					<Value>homoenhance</Value>
					<Label>Homogeneous on enhanced CT</Label>
				</Choice>
				<Choice>
					<Value>heter</Value>
					<Label>Heterogeneous</Label>
				</Choice>
			</ChoiceInfo>
			<ConditionalProperties>
			<ConditionalProperty>
				<EqualCondition DataElementId="catfat" ComparisonValue="yes"/>
				 <IsRelevant>false</IsRelevant>					
				</ConditionalProperty>	
             </ConditionalProperties>
	</ChoiceDataElement>
 
	<ChoiceDataElement Id="catThomo" DisplaySequence="3">
