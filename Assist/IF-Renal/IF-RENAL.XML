<Rules>
  <DecisionPoint Id="rule1">
     <Label>THomo</Label>
      <Branch>
		 <Label>labelone</Label>			  
				  <EqualCondition DataElementId="catrenalmass" ComparisonValue="Thomo"/> 
				  <DecisionPoint Id="rule2">	 
				  <Label>ThomoTSTC</Label>
					<Branch>
					   <Label>Thomob</Label>
						 <EqualCondition DataElementId="catThomo" ComparisonValue="benigncys"/>
						    <EndPointRef EndPointId="endpointbenign" ></EndPointRef>	
                       </Branch>
					<Branch>
					 <Label>Thomoi</Label>
					  <EqualCondition DataElementId="catThomo" ComparisonValue="inconclu"/>
				      <EndPointRef EndPointId="endpointfollowup"></EndPointRef>		
                    </Branch>
					</DecisionPoint>
				
       </Branch>
 </DecisionPoint>
 
 
 <DecisionPoint Id="rule3">
     <Label>Homotwo</Label>
      <Branch>
		 <Label>labeltwo</Label>			  
				  <EqualCondition DataElementId="catrenalmass" ComparisonValue="homo"/> 
				  <DecisionPoint Id="rule4">	 
				  <Label>homotwol</Label>
					<Branch>
					   <Label>homolow</Label>
						 <EqualCondition DataElementId="cathomo" ComparisonValue="homolower"/>
						    <EndPointRef EndPointId="endpointbenign" ></EndPointRef>	
                       </Branch>
					<Branch>
					 <Label>homomi</Label>
					  <EqualCondition DataElementId="cathomo" ComparisonValue="homomid"/>
				      <EndPointRef EndPointId="endpointrecommend"></EndPointRef>		
                    </Branch>
					<Branch>
					 <Label>homohi</Label>
					  <EqualCondition DataElementId="cathomo" ComparisonValue="homoupper"/>
				      <EndPointRef EndPointId="endpointhomo"></EndPointRef>		
                    </Branch>
					</DecisionPoint>
				
       </Branch>
 </DecisionPoint>
 
 <DecisionPoint Id="rule5">	 
    <Label>heterocalci</Label>
					<Branch>
					   <Label>heterofollow</Label>
						 <EqualCondition DataElementId="catrenalmass" ComparisonValue="heter"/>
						    <EndPointRef EndPointId="endpointrecommend" ></EndPointRef>	
                       </Branch>
  </DecisionPoint>
  
  
    <DecisionPoint Id="rule6">
     <Label>CThomo</Label>
      <Branch>
		 <Label>CThomolabel</Label>			  
				  <EqualCondition DataElementId="catrenalmass" ComparisonValue="homoenhance"/> 
				  <DecisionPoint Id="rule2">	 
				  <Label>homoenhancetwo</Label>
					<Branch>
					   <Label>homeoenhancethree</Label>
						 <EqualCondition DataElementId="cystictype" ComparisonValue="benigncystmass"/>
						    <EndPointRef EndPointId="endpointbenign" ></EndPointRef>	
                       </Branch>
					<Branch>
					 <Label>Thomoenhancefour</Label>
					  <EqualCondition DataElementId="cystictype" ComparisonValue="solidcystmass"/>
				      <EndPointRef EndPointId="endpointCT"></EndPointRef>		
                    </Branch>
					</DecisionPoint>
				
       </Branch>
 </DecisionPoint>
 <DecisionPoint Id="rule1b">	 
    <Label>rulebenignb</Label>
			<Branch>
				<Label>labelonetwob</Label>
						 <EqualCondition DataElementId="bosniak" ComparisonValue="onetwoclass"/>
						    <EndPointRef EndPointId="endpointbenignsimple" ></EndPointRef>	
            </Branch>
  </DecisionPoint>
  
  
 <DecisionPoint Id="rule2b">	 
    <Label>rulecomplexb</Label>
			<Branch>
				<Label>labeltwofb</Label>
						 <EqualCondition DataElementId="bosniak" ComparisonValue="twofclass"/>
						    <EndPointRef EndPointId="endpointfollowup5" ></EndPointRef>	
            </Branch>
  </DecisionPoint>
  
  
  <DecisionPoint Id="rule3b">	 
    <Label>rulenomorpho</Label>
			<Branch>
				<Label>labelnomorpho</Label>
						 <EqualCondition DataElementId="morphotype" ComparisonValue="yes"/>
						    <EndPointRef EndPointId="endpointbenigncomplex" ></EndPointRef>	
            </Branch>
  </DecisionPoint>
  
  
  <DecisionPoint Id="rule4b">	 
    <Label>rulemorpho</Label>
			<Branch>
				<Label>labelmorpho</Label>
						 <EqualCondition DataElementId="morphotype" ComparisonValue="no"/>
						    <EndPointRef EndPointId="endpointmanage" ></EndPointRef>	
            </Branch>
  </DecisionPoint>
  
  
  <DecisionPoint Id="rule5b">	 
