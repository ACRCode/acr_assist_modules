 </DataElements>
 
 
 
 <Rules>
	<DecisionPoint Id="IF-Liver">
	<Label>IF-Liver</Label>
	<Branch>
		<Label>Benign</Label>
		<OrCondition>
			<EqualCondition DataElementId="Patientriskcategory" ComparisonValue="Lowriskpatient"/>
			<EqualCondition DataElementId="Patientriskcategory2" ComparisonValue="Lowriskpatient"/>
			</OrCondition>	
			<EndPointRef EndPointId="Benign"></EndPointRef>
		
	 </Branch>
	 
	 <Branch>
		<Label>Follow</Label>
			<EqualCondition DataElementId="Patientriskcategory" ComparisonValue="Highriskpatient"/>
			<EndPointRef EndPointId="Follow"></EndPointRef>
	 </Branch>
	
	<Branch>
		<Label>Benign1</Label>
		<OrCondition>
			<EqualCondition DataElementId="Imagingfeatures" ComparisonValue="Benignimagingfeatures"/>
			<EqualCondition DataElementId="Imagingfeatures2" ComparisonValue="Benignimagingfeatures"/>
			</OrCondition>
			<EndPointRef EndPointId="Benign1"></EndPointRef>
	 </Branch>
	 
	 <Branch>
		<Label>Suspicious</Label>
		<AndCondition>
			<EqualCondition DataElementId="Imagingfeatures" ComparisonValue="Suspiciousimagingfeatures"/>
			<EqualCondition DataElementId="Imagingfeatures2" ComparisonValue="Suspiciousimagingfeatures"/>

         </AndCondition>
			<EndPointRef EndPointId="Suspicious"></EndPointRef>
	 </Branch>
	 
	 <Branch>
		<Label>Suspicious</Label>
			<EqualCondition DataElementId="Imagingfeatures" ComparisonValue="Flashfillingimagingfeature"/>
			<EndPointRef EndPointId="Suspicious"></EndPointRef>
	 </Branch>
	 
	
	 
	 
	 <Branch>
		<Label>High_risk</Label>
		   <AndCondition>
			<EqualCondition DataElementId="Patientriskcategory2" ComparisonValue="Highriskpatient"/>
           </AndCondition>
			<EndPointRef EndPointId="High_risk"></EndPointRef>
	 </Branch>
	 
	 
	 </DecisionPoint>
    </Rules>

	
   <EndPoints>
			
	 <EndPoint Id="Benign">
	  	<Label>Benign</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="PlainText">Benign, no further follow-up.</ReportText>
				</Branch>
			</ReportSection>
		</ReportSections>
      </EndPoint>
	  
	  <EndPoint Id="Follow">
	  	<Label>Follow</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="PlainText">Follow-up MRI in 3-6 months.</ReportText>
				</Branch>
			</ReportSection>
		</ReportSections>
      </EndPoint>
	  
	  <EndPoint Id="Benign1">
	  	<Label>Benign1</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="PlainText">Benign, no further follow-up.</ReportText>
				</Branch>
			</ReportSection>
		</ReportSections>
      </EndPoint>
	  
	 
	  
	<EndPoint Id="Suspicious">
	  	<Label>Suspicious</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="PlainText">Hepatic MRI now.</ReportText>
				</Branch>
			</ReportSection>
		</ReportSections>
      </EndPoint>	
	 
	
	<EndPoint Id="High_risk">
	  	<Label>High_risk</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="PlainText">Hepatic MRI now or biopsy.</ReportText>
				</Branch>
			</ReportSection>
		</ReportSections>
      </EndPoint>		
	</EndPoints>



	

