<DataElements>


<ChoiceDataElement Id="Incidentallivermasssize" IsRequired="true" DisplaySequence="1">
<Label>Incidental liver mass size </Label>
<ChoiceInfo>
<Choice>
<Value>1cm</Value>
<Label> &lt; 1 cm </Label>
 </Choice>
<Choice>
<Value>1015cm</Value>
<Label>1.0 - 1.5 cm </Label>
 </Choice>
<Choice>
<Value>15cm</Value>
<Label> &gt; 1.5 cm </Label>
 </Choice>
 </ChoiceInfo>
 </ChoiceDataElement>
 
 
<ChoiceDataElement Id="Patientriskcategory" IsRequired="true" DisplaySequence="2">
<Label>Patient risk category</Label>
<ChoiceInfo>
<Choice>
<Value>Lowriskpatient</Value>
<Label>Low-risk patient</Label>
 </Choice>
<Choice>
<Value>Highriskpatient</Value>
<Label>High-risk patient </Label>
 </Choice>
 </ChoiceInfo>
 <ConditionalProperties>				
				<ConditionalProperty>
					<OrCondition>		
						<EqualCondition DataElementId="Incidentallivermasssize" ComparisonValue="1015cm"/>
						<EqualCondition DataElementId="Incidentallivermasssize" ComparisonValue="15cm"/>
					</OrCondition>
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
			</ConditionalProperties>
 </ChoiceDataElement>
 
 
<ChoiceDataElement Id="Imagingfeatures" IsRequired="true" DisplaySequence="3">
<Label>Imaging features</Label>
<ChoiceInfo>
<Choice>
<Value>Benignimagingfeatures</Value>
<Label>Benign imaging features </Label>
 </Choice>
<Choice>
<Value>Suspiciousimagingfeatures</Value>
<Label>Suspicious imaging features </Label>
 </Choice>
<Choice>
<Value>Flashfillingimagingfeature</Value>
<Label>&apos;Flash-filling&apos; imaging feature </Label>
 </Choice>
 </ChoiceInfo>
 <ConditionalProperties>				
				<ConditionalProperty>
					<OrCondition>		
						<EqualCondition DataElementId="Incidentallivermasssize" ComparisonValue="1cm"/>
						<EqualCondition DataElementId="Incidentallivermasssize" ComparisonValue="15cm"/>
					</OrCondition>
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
			</ConditionalProperties>
 </ChoiceDataElement>
 
 <ChoiceDataElement Id="Imagingfeatures2" IsRequired="true" DisplaySequence="4">
<Label>Imaging features</Label>
<ChoiceInfo>
<Choice>
<Value>Benignimagingfeatures</Value>
<Label>Benign imaging features </Label>
 </Choice>
 <Choice>
<Value>SusporFlashfillingimagingfeature</Value>
<Label>Suspicious or &apos;Flash-filling&apos; imaging feature </Label>
 </Choice>
