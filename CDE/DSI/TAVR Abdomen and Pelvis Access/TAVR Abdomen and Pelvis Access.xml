<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>TAVR Abdomen and Pelvis Access</Label>
		<ID>TAVR Abdomen and Pelvis Access</ID>
		<SchemaVersion>1.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<Info>
			<Description>This module describes the Common Data elements and Macros for TAVR Abdomen and Pelvis Access</Description>
			
			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>
			
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.jpg</Location>
					<Label>TAVR Abdomen and Pelvis Access key image</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>C</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>
		
	</Metadata>


  <DataElements>
  
   <GlobalValue Id="conditionConst">10</GlobalValue>
  
   <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
			<Label>Age of the patient</Label>
			<Minimum>0.00</Minimum>
			<Maximum>105.00</Maximum>
		</NumericDataElement>
		
		 <ChoiceDataElement Id="Sex" IsRequired="true" DisplaySequence="2">
			<Label>Sex of the patient</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Female</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Male</Label>
				</Choice>			
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<NumericDataElement Id="min" IsRequired="true" DisplaySequence="3">
			<Label>Arterial minimal diameters in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="dia" IsRequired="true" DisplaySequence="4">
			<Label>Minimum diameter of the aorta below the renal arteries in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="right" IsRequired="true" DisplaySequence="5">
			<Label>Minimum diameter of the right common iliac artery</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="left" IsRequired="true" DisplaySequence="6">
			<Label>Minimum diameter of the left common iliac artery in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="ext" IsRequired="true" DisplaySequence="7">
			<Label>Minimum diameter of the right external iliac artery in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="left_ext" IsRequired="true" DisplaySequence="8">
			<Label>Minimum diameter of the left external iliac artery in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="common" IsRequired="true" DisplaySequence="9">
			<Label>Minimum diameter of the right common femoral artery in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>
		
		<NumericDataElement Id="fem" IsRequired="true" DisplaySequence="10">
			<Label>Minimum diameter of the left common femoral artery in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1000.00</Maximum>
		</NumericDataElement>

   </DataElements>
   
  <Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>		
	</Rules>

	
    <EndPoints>		
	 <TemplatePartial Id ="ExamPartial">
	 	<Branch>
	 		<ReportText Type="PlainText">EXAM TYPE: TAVR Abdomen and Pelvis Access Evaluation</ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText" >COMPARISON: NONE </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 	</Branch>
	 </TemplatePartial>


	<EndPoint Id="macroEndpoint">
		<Label>A</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="InsertPartialTemplate" Value="ExamPartial"></ReportText>
					<ReportText Type="Newline"/>
					<ReportText Type="PlainText">TAVR Abdomen and Pelvis Access evaluation results:</ReportText>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="Age" />
					<ReportText Type="PlainText"> Patient's age: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="Age"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="Sex" />
					<ReportText Type="PlainText"> Patient's sex: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="Sex"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="min" />
					<ReportText Type="PlainText"> Arterial minimal diameters: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="min"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="dia" />
					<ReportText Type="PlainText"> Minimum diameter of the aorta below the renal arteries: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="dia"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="right" />
					<ReportText Type="PlainText"> Minimum diameter of the right common iliac artery: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="right"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				<Branch>
				    <SectionIf DataElementId="left" />
					<ReportText Type="PlainText"> Minimum diameter of the left common iliac artery: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="left"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				<Branch>
				    <SectionIf DataElementId="ext" />
					<ReportText Type="PlainText"> Minimum diameter of the right external iliac artery: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="ext"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				<Branch>
				    <SectionIf DataElementId="left_ext" />
					<ReportText Type="PlainText"> Minimum diameter of the left external iliac artery: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="left_ext"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				<Branch>
				    <SectionIf DataElementId="common" />
					<ReportText Type="PlainText"> Minimum diameter of the right common femoral artery: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="common"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				<Branch>
				    <SectionIf DataElementId="fem" />
					<ReportText Type="PlainText"> Minimum diameter of the left common femoral artery: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="fem"/>
					<ReportText Type="Newline"/>
				</Branch>

				</ReportSection>
		</ReportSections>
		</EndPoint>	
	</EndPoints>
	</ReportingModule>