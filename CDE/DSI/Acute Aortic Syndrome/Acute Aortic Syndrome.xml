<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>Acute Aortic Syndrome</Label>
		<ID>Acute Aortic Syndrome</ID>
		<SchemaVersion>1.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<Info>
			<Description>This module describes the Common Data elements and Macros for Acute Aortic Syndrome</Description>
			
			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>
			
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.jpg</Location>
					<Label>Acute Aortic Syndrome key image</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>C</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>
		
	</Metadata>


  <DataElements>
  
   <GlobalValue Id="conditionConst">10</GlobalValue>
  
   <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
			<Label>Age of the patient</Label>
			<Minimum>20.00</Minimum>
			<Maximum>120.00</Maximum>
		</NumericDataElement>
	
	<ChoiceDataElement Id="dissection" IsRequired="true" DisplaySequence="2">
      <Label>Aortic Dissection</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Acute</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Subacute</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Chronic</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="ischemia" IsRequired="true" DisplaySequence="3">
      <Label>Ischemia</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Abdominal organ ischemia</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Limb ischemia</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Ischemic stroke</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>Embolic stroke</Label>
        </Choice>
        <Choice>
          <Value>4</Value>
          <Label>Paraplegia</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="comorbidities" IsRequired="true" DisplaySequence="3">
      <Label>Comorbidities</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Hypertension</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Inherited connective tissue disorder</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Atherosclerosis</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>Vasculitis</Label>
        </Choice>
        <Choice>
          <Value>4</Value>
          <Label>Coronary artery disease</Label>
        </Choice>
		<Choice>
          <Value>5</Value>
          <Label>Chronic obstructive pulmonary disease</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="emergencies" IsRequired="true" DisplaySequence="4">
      <Label>Aortic emergencies</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Aortic dissection</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Aortic intramural hematoma</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Penetrating atherosclerotic ulcer</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>Aortic aneurysmal rupture</Label>
        </Choice>
        <Choice>
          <Value>4</Value>
          <Label>Traumatic thoracic aortic injury</Label>
        </Choice>
		<Choice>
          <Value>5</Value>
          <Label>Traumatic abdominal aortic injury</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="smoking" IsRequired="true" DisplaySequence="5">
      <Label>Smoking history</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Yes</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>No</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="rupture" IsRequired="true" DisplaySequence="6">
      <Label>Aortic aneurysmal rupture</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Intraperitoneal rupture</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Retroperitoneal rupture</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Aortocaval fistula</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>Primary aorto-enteric fistula</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="view" IsRequired="true" DisplaySequence="7">
      <Label>CT Views</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Axial</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Coronal</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Sagittal</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="acute" IsRequired="true" DisplaySequence="8">
      <Label>Acute aortic syndrome</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>Acute aortic syndrome absent</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>Acute aortic syndrome present</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="stanford" IsRequired="true" DisplaySequence="9">
      <Label>Stanford classification</Label>
      <ChoiceInfo>
	  <Choice>
          <Value>0</Value>
          <Label>A</Label>
        </Choice>
		<Choice>
          <Value>1</Value>
          <Label>B</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	
	    
   </DataElements>
   
  <Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>		
	</Rules>

	
    <EndPoints>		
	 <TemplatePartial Id ="ExamPartial">
	 	<Branch>
	 		<ReportText Type="PlainText">EXAM TYPE: Acute Aortic Syndrome Evaluation</ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText" >COMPARISON: NONE </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 	</Branch>
	 </TemplatePartial>


	<EndPoint Id="macroEndpoint">
		<Label>A</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="InsertPartialTemplate" Value="ExamPartial"></ReportText>
					<ReportText Type="Newline"/>
					<ReportText Type="PlainText">Acute Aortic Syndrome evaluation results:</ReportText>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="Age" />
					<ReportText Type="PlainText"> Patient's age: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="Age"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="dissection" />
					<ReportText Type="PlainText"> Aortic Dissection: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="dissection"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="ischemia" />
					<ReportText Type="PlainText"> Ischemia: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="ischemia"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="comorbidities" />
					<ReportText Type="PlainText"> Comorbidities: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="comorbidities"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="emergencies" />
					<ReportText Type="PlainText"> Aortic emergencies: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="emergencies"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="smoking" />
					<ReportText Type="PlainText"> Smoking history: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="smoking"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="rupture" />
					<ReportText Type="PlainText"> Aortic aneurysmal rupture: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="rupture"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="view" />
					<ReportText Type="PlainText"> Views: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="view"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="acute" />
					<ReportText Type="PlainText"> Acute aortic syndrome: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="acute"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="stanford" />
					<ReportText Type="PlainText"> Stanford classification: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="stanford"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				
				</ReportSection>
		</ReportSections>
		</EndPoint>	
	</EndPoints>
	</ReportingModule>