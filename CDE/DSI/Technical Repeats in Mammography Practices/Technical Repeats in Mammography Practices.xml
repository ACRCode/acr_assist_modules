<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>Technical Repeats in Mammography Practices</Label>
		<ID>Technical Repeats in Mammography Practices</ID>
		<SchemaVersion>1.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<Info>
			<Description>This module describes the Common Data elements and Macros for Technical Repeats in Mammography Practices</Description>
			
			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>
			
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.jpg</Location>
					<Label>Technical Repeats in Mammography Practices key image</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>C</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>
		
	</Metadata>


  <DataElements>
  
   <GlobalValue Id="conditionConst">10</GlobalValue>
  
   <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
			<Label>Age of the patient</Label>
			<Minimum>40.00</Minimum>
			<Maximum>120.00</Maximum>
		</NumericDataElement>
	
	<ChoiceDataElement Id="views" IsRequired="true" DisplaySequence="2">
      <Label>Mammography Views</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>CC</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>MLO</Label>
        </Choice>
		<Choice>
          <Value>2</Value>
          <Label>LM</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>XCCM</Label>
        </Choice>
		<Choice>
          <Value>4</Value>
          <Label>XCCL</Label>
        </Choice>
		<Choice>
          <Value>5</Value>
          <Label>Cleavage view</Label>
        </Choice>
		<Choice>
          <Value>6</Value>
          <Label>Other</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	
	
	<ChoiceDataElement Id="fat" IsRequired="true" DisplaySequence="3">
      <Label>Retropectoral Fat Plane Visible on CC</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Retropectoral fat plane not visible</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Retropectoral fat plane partially in view</Label>
        </Choice>
		<Choice>
          <Value>2</Value>
          <Label>Nipple partially identified but inappropriately positioned</Label>
        </Choice>
		<Choice>
          <Value>3</Value>
          <Label>Retropectoral fat plane fully in view</Label>
        </Choice>
		<Choice>
          <Value>4</Value>
          <Label>Unknown</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
  
	
	<ChoiceDataElement Id="nipple" IsRequired="true" DisplaySequence="4">
      <Label>Nipple In Profile</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Nipple not visible</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Nipple visible</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="blur" IsRequired="true" DisplaySequence="5">
      <Label>Motion Blur</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No motion blur</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Image contains motion blur</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="posterior" IsRequired="true" DisplaySequence="6">
      <Label>Adequate visualization of posterior breast tissue</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Inadequate visualization</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Adequate visualization</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="sagging" IsRequired="true" DisplaySequence="7">
      <Label>Sagging of the breast on MLO View</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Acceptable position</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Excessive sagging</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="fold" IsRequired="true" DisplaySequence="8">
      <Label>Inframammary Fold On MLO</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Inframammary fold not visible</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Inframammary fold visible</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="image" IsRequired="true" DisplaySequence="9">
      <Label>Breast height on Image Receptor</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Acceptable breast height</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Breast too high</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<NumericDataElement Id="cm" IsRequired="true" DisplaySequence="10">
			<Label>Posterior nipple line on the CC view measured in cm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>1.00</Maximum>
		</NumericDataElement>
	
	<ChoiceDataElement Id="skin" IsRequired="true" DisplaySequence="11">
      <Label>Skin folds obscuring visualization</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No skin folds</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Skin folds may obscure visualization</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="excess" IsRequired="true" DisplaySequence="12">
      <Label>Excessive Exaggeration</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No excessive exaggeration</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Excessive exaggeration</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="other" IsRequired="true" DisplaySequence="13">
      <Label>Other Body Parts Obscuring Image</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No other body parts</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Other body parts present</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="off" IsRequired="true" DisplaySequence="14">
      <Label>Breast Cut off</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No breast cut off in image</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Breast cutoff in image</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="additional" IsRequired="true" DisplaySequence="15">
      <Label>Additional View Necessary</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>No further imaging necessary</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Further imaging necessary</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Unknown</Label>
        </Choice>	
      </ChoiceInfo>
    </ChoiceDataElement>
	
	
   </DataElements>
   
  <Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>		
	</Rules>

	
    <EndPoints>		
	 <TemplatePartial Id ="ExamPartial">
	 	<Branch>
	 		<ReportText Type="PlainText">EXAM TYPE: Technical Repeats in Mammography Practices Evaluation</ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText" >COMPARISON: NONE </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 	</Branch>
	 </TemplatePartial>


	<EndPoint Id="macroEndpoint">
		<Label>A</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="InsertPartialTemplate" Value="ExamPartial"></ReportText>
					<ReportText Type="Newline"/>
					<ReportText Type="PlainText">Technical Repeats in Mammography Practices evaluation results:</ReportText>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="Age" />
					<ReportText Type="PlainText"> Patient's age: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="Age"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="views" />
					<ReportText Type="PlainText"> Mammography Views: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="views"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="fat" />
					<ReportText Type="PlainText"> Retropectoral Fat Plane Visible on CC: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="fat"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="nipple" />
					<ReportText Type="PlainText"> Nipple In Profile: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="nipple"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="blur" />
					<ReportText Type="PlainText"> Motion Blur: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="blur"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="posterior" />
					<ReportText Type="PlainText"> Adequate visualization of posterior breast tissue: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="posterior"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="sagging" />
					<ReportText Type="PlainText"> Sagging of the breast on MLO View: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="sagging"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="fold" />
					<ReportText Type="PlainText"> Inframammary Fold On MLO: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="fold"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="image" />
					<ReportText Type="PlainText"> Breast height on Image Receptor: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="image"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="cm" />
					<ReportText Type="PlainText"> Posterior nipple line on the CC view: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="cm"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="skin" />
					<ReportText Type="PlainText"> Skin folds obscuring visualization: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="skin"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="excess" />
					<ReportText Type="PlainText">Excessive Exaggeration: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="excess"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="other" />
					<ReportText Type="PlainText">Other Body Parts Obscuring Image: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="other"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="off" />
					<ReportText Type="PlainText">Breast Cut off: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="off"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="additional" />
					<ReportText Type="PlainText">Additional View Necessary: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="additional"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				</ReportSection>
		</ReportSections>
		</EndPoint>	
	</EndPoints>
	</ReportingModule>