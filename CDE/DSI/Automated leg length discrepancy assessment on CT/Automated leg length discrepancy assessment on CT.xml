<?xml version="1.0" encoding="utf-8"?>
<ReportingModule>
  <Metadata>
    <Label>Automated leg length discrepancy assessment on CT</Label>
    <ID>Automated leg length discrepancy assessment on CT</ID>
    <SchemaVersion>1.0</SchemaVersion>
    <ModuleVersion>1.0</ModuleVersion>
    <CodableConcept Name="codableConcept_Automated leg length discrepancy assessment on CT">
      <Coding>
        <System Value="RadElement" />
        <Version Value="1.0" />
        <Code Value="RDES134" />
        <Display Value="RDES134" />
        <UserSelected Value="false" />
        <Url Value="http://radelement.org/set/RDES134" />
      </Coding>
    </CodableConcept>
    <Info>
      <Description>This module describes the Common Data elements and Macros for Calcium Scoring of the Automated leg length discrepancy assessment on CT</Description>
      <References>
        <Citation Url="http://radelement.org/"> CDE URL </Citation>
      </References>
      <Diagrams>
        <Diagram DisplaySequence="1" IsKeyDiagram="true">
          <Location>keydiagram.jpg</Location>
          <Label>Automated leg length discrepancy assessment on CT key image</Label>
        </Diagram>
      </Diagrams>
      <Contact>
        <Name>ACR Assist</Name>
        <Email>acr-assist@acr.org</Email>
        <Institution>American College of Radiology</Institution>
      </Contact>
    </Info>
    <ReportCitationText>C</ReportCitationText>
    <ApplicableSexes Value="Both" />
  </Metadata>
  <DataElements>
    <GlobalValue Id="conditionConst">10</GlobalValue>
    <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
      <CodableConcept Name="codableConcept_RDES134_Age">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE878" />
          <Display Value="RDE878" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE878" />
        </Coding>
      </CodableConcept>
      <Label>Age of the patient</Label>
      <Minimum>0.00</Minimum>
      <Maximum>90.00</Maximum>
    </NumericDataElement>
    <ChoiceDataElement Id="Sex" IsRequired="true" DisplaySequence="2">
      <CodableConcept Name="codableConcept_RDES134_Sex">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE879" />
          <Display Value="RDE879" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE879" />
        </Coding>
      </CodableConcept>
      <Label>Sex of the patient</Label>
      <ChoiceInfo>
        <Choice>
          <Value>1</Value>
          <Label>Female</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Male</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
    <ChoiceDataElement Id="focus" IsRequired="true" DisplaySequence="3">
      <CodableConcept Name="codableConcept_RDES134_focus">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE880" />
          <Display Value="RDE880" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE880" />
        </Coding>
      </CodableConcept>
      <Label>Anatomic focus</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Hip</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Thigh</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Knee</Label>
        </Choice>
        <Choice>
          <Value>3</Value>
          <Label>Leg</Label>
        </Choice>
        <Choice>
          <Value>4</Value>
          <Label>Ankle</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
    <NumericDataElement Id="dist" IsRequired="true" DisplaySequence="4">
      <CodableConcept Name="codableConcept_RDES134_dist">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE881" />
          <Display Value="RDE881" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE881" />
        </Coding>
      </CodableConcept>
      <Label>Distance from top of femoral head to mid tibial plafond in cm</Label>
      <Minimum>0.00</Minimum>
      <Maximum>190.00</Maximum>
    </NumericDataElement>
    <NumericDataElement Id="a" IsRequired="true" DisplaySequence="5">
      <CodableConcept Name="codableConcept_RDES134_a">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE882" />
          <Display Value="RDE882" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE882" />
        </Coding>
      </CodableConcept>
      <Label>Distance from top of femoral head to medial condyle articular surfacein cm</Label>
      <Minimum>0.00</Minimum>
      <Maximum>190.00</Maximum>
    </NumericDataElement>
    <NumericDataElement Id="b" IsRequired="true" DisplaySequence="6">
      <CodableConcept Name="codableConcept_RDES134_b">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE883" />
          <Display Value="RDE883" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE883" />
        </Coding>
      </CodableConcept>
      <Label> Distance from top of tibial intercondylar eminence to the mid-tibial plafond in cm</Label>
      <Minimum>0.00</Minimum>
      <Maximum>190.00</Maximum>
    </NumericDataElement>
    <NumericDataElement Id="c" IsRequired="true" DisplaySequence="7">
      <CodableConcept Name="codableConcept_RDES134_c">
        <Coding>
          <System Value="RadElement" />
          <Version Value="1.0" />
          <Code Value="RDE884" />
          <Display Value="RDE884" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE884" />
        </Coding>
      </CodableConcept>
      <Label>Femur length + tibia length in cm</Label>
      <Minimum>0.00</Minimum>
      <Maximum>190.00</Maximum>
    </NumericDataElement>
  </DataElements>
  <Rules>
    <DecisionPoint Id="macrodp">
      <Label>Macro Banch</Label>
      <Branch>
        <EqualCondition DataElementId="conditionConst" ComparisonValue="10" />
        <EndPointRef EndPointId="macroEndpoint" />
      </Branch>
    </DecisionPoint>
  </Rules>
  <EndPoints>
    <TemplatePartial Id="ExamPartial">
      <Branch>
        <ReportText Type="PlainText">EXAM TYPE: Automated leg length discrepancy assessment on CT Evaluation</ReportText>
        <ReportText Type="Newline" />
        <ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
        <ReportText Type="Newline" />
        <ReportText Type="PlainText">COMPARISON: NONE </ReportText>
        <ReportText Type="Newline" />
      </Branch>
    </TemplatePartial>
    <EndPoint Id="macroEndpoint">
      <Label>A</Label>
      <ReportSections>
        <ReportSection SectionId="findings">
          <Branch>
            <ReportText Type="InsertPartialTemplate" Value="ExamPartial" />
            <ReportText Type="Newline" />
            <ReportText Type="PlainText">Automated leg length discrepancy assessment on CT evaluation results:</ReportText>
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="Age" />
            <ReportText Type="PlainText"> Patient's age: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="Age" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="Sex" />
            <ReportText Type="PlainText"> Patient's sex: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="Sex" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="focus" />
            <ReportText Type="PlainText"> Anatomical focus: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="focus" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="dist" />
            <ReportText Type="PlainText"> Distance from top of femoral head to mid tibial plafond: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="dist" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="a" />
            <ReportText Type="PlainText"> Distance from top of femoral head to medial condyle articular surface: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="a" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="b" />
            <ReportText Type="PlainText"> Distance from top of tibial intercondylar eminence to the mid-tibial plafond: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="b" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="c" />
            <ReportText Type="PlainText"> Femur length + tibia length: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="c" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="" />
            <ReportText Type="PlainText"> : </ReportText>
            <ReportText Type="InsertDataElementValue" Value="" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="" />
            <ReportText Type="PlainText"> : </ReportText>
            <ReportText Type="InsertDataElementValue" Value="" />
            <ReportText Type="Newline" />
          </Branch>
        </ReportSection>
      </ReportSections>
    </EndPoint>
  </EndPoints>
</ReportingModule>