      <ChoiceInfo>
        <Choice>
          <Value>1</Value>
          <Label>Female</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Male</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
    <NumericDataElement Id="peri" IsRequired="true" DisplaySequence="3">
