  <Rules>
    <DecisionPoint Id="macrodp">
      <Label>Macro Banch</Label>
      <Branch>
        <EqualCondition DataElementId="conditionConst" ComparisonValue="10" />
        <EndPointRef EndPointId="macroEndpoint" />
      </Branch>
    </DecisionPoint>
  </Rules>
  <EndPoints>
    <TemplatePartial Id="ExamPartial">
      <Branch>
        <ReportText Type="PlainText">EXAM TYPE: Evaluation of Automated Volumetric Quantification of Pericardial Effusion on CCTA</ReportText>
        <ReportText Type="Newline" />
        <ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
        <ReportText Type="Newline" />
        <ReportText Type="PlainText">COMPARISON: NONE </ReportText>
        <ReportText Type="Newline" />
      </Branch>
    </TemplatePartial>
    <EndPoint Id="macroEndpoint">
      <Label>A</Label>
      <ReportSections>
        <ReportSection SectionId="findings">
          <Branch>
            <ReportText Type="InsertPartialTemplate" Value="ExamPartial" />
            <ReportText Type="Newline" />
            <ReportText Type="PlainText">Automated Volumetric Quantification of Pericardial Effusion on CCTA evaluation results:</ReportText>
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="Age" />
            <ReportText Type="PlainText"> Patient's age: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="Age" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="Sex" />
            <ReportText Type="PlainText"> Patient's sex: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="Sex" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="peri" />
            <ReportText Type="PlainText"> Pericardium thickness: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="peri" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="spread" />
            <ReportText Type="PlainText"> Spread: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="spread" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="ratio" />
            <ReportText Type="PlainText"> Cardiothoracic ratio: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="ratio" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="edema" />
            <ReportText Type="PlainText"> Pulmonary edema: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="edema" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="lesion" />
            <ReportText Type="PlainText"> Pericardial lesions: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="lesion" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="const" />
            <ReportText Type="PlainText"> Constrictive pericarditis: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="const" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="pev" />
            <ReportText Type="PlainText"> Pericardial effusion volume: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="pev" />
            <ReportText Type="Newline" />
          </Branch>
          <Branch>
            <SectionIf DataElementId="ave" />
            <ReportText Type="PlainText"> Average density of the effusion: </ReportText>
            <ReportText Type="InsertDataElementValue" Value="ave" />
            <ReportText Type="Newline" />
          </Branch>
        </ReportSection>
      </ReportSections>
    </EndPoint>
  </EndPoints>
</ReportingModule>