<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>Breast Cancer Risk Assessment</Label>
		<ID>Breast Cancer Risk Assessment</ID>
		<SchemaVersion>1.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<Info>
			<Description>This module describes the Common Data elements and Macros for Breast Cancer Risk Assessment</Description>
			
			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>
			
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.jpg</Location>
					<Label>Breast Cancer Risk Assessment key image</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>C</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>
		
	</Metadata>


  <DataElements>
  
   <GlobalValue Id="conditionConst">10</GlobalValue>
  
   <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
			<Label>Age of the patient</Label>
			<Minimum>40.00</Minimum>
			<Maximum>120.00</Maximum>
		</NumericDataElement>
		
		<ChoiceDataElement Id="history" IsRequired="true" DisplaySequence="2">
      <Label>Previous history</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Prior surgery</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Implants</Label>
        </Choice>
		<Choice>
          <Value>2</Value>
          <Label>Not significant</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="views" IsRequired="true" DisplaySequence="3">
      <Label>Views</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Standard craniocaudal</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label> Standard mediolateral</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
  
  <ChoiceDataElement Id="historic" IsRequired="true" DisplaySequence="4">
      <Label>Historical data from EMR</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Parity</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Estrogen use</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Menopause</Label>
        </Choice>
         <Choice>
          <Value>2</Value>
          <Label>Obesity</Label>
        </Choice>
          <Choice>
          <Value>3</Value>
          <Label>Other</Label>
        </Choice>		
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="historic1" IsRequired="true" DisplaySequence="5">
      <Label>Additional history</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Family cancer history</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Menarche</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Personal cancer history</Label>
        </Choice>
         <Choice>
          <Value>3</Value>
          <Label>Other</Label>
        </Choice>		
      </ChoiceInfo>
    </ChoiceDataElement>
	
	<ChoiceDataElement Id="risk" IsRequired="true" DisplaySequence="6">
      <Label>Risk assessment calculators</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Gail</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Tyrer Cuzick</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>BRCA pro</Label>
        </Choice>
          <Choice>
          <Value>3</Value>
          <Label>Other</Label>
        </Choice>		
      </ChoiceInfo>
    </ChoiceDataElement>
	
	
	<NumericDataElement Id="personal" IsRequired="true" DisplaySequence="7">
			<Label>Personal lifetime risk assessment for breast cancer in probability of developing breast cancer</Label>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
		</NumericDataElement>
	
	<ChoiceDataElement Id="best" IsRequired="true" DisplaySequence="8">
      <Label>Best screening interval for patient</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>MBI</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>MRI</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>CEDM</Label>
        </Choice>
		 <Choice>
          <Value>3</Value>
          <Label>Other</Label>
        </Choice>
      </ChoiceInfo>
    </ChoiceDataElement>
  
  
       
   </DataElements>
   
  <Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>		
	</Rules>

	
    <EndPoints>		
	 <TemplatePartial Id ="ExamPartial">
	 	<Branch>
	 		<ReportText Type="PlainText">EXAM TYPE: Breast Cancer Risk Assessment EVALUATION</ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText">INDICATION: detailed examination </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 		<ReportText Type="PlainText" >COMPARISON: NONE </ReportText>
	 		<ReportText Type="Newline"></ReportText>
	 	</Branch>
	 </TemplatePartial>


	<EndPoint Id="macroEndpoint">
		<Label>A</Label>
		<ReportSections>
			<ReportSection SectionId="findings">
				<Branch>
					<ReportText Type="InsertPartialTemplate" Value="ExamPartial"></ReportText>
					<ReportText Type="Newline"/>
					<ReportText Type="PlainText">Breast Cancer Risk Assessment evaluation results:</ReportText>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="Age" />
					<ReportText Type="PlainText"> Patient's age: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="Age"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="history" />
					<ReportText Type="PlainText"> Previous history: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="history"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="views" />
					<ReportText Type="PlainText"> Views: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="views"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="historic" />
					<ReportText Type="PlainText"> Historical data from EMR: </ReportText> 
					<ReportText Type="InsertDataElementValue" Value="historic"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="historic1" />
					<ReportText Type="PlainText"> Additional history: </ReportText>   
					<ReportText Type="InsertDataElementValue" Value="historic1"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="risk" />
					<ReportText Type="PlainText"> Risk assessment calculators: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="risk"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="personal" />
					<ReportText Type="PlainText">Personal lifetime risk assessment for breast cancer: </ReportText>
					<ReportText Type="InsertDataElementValue" Value="personal"/>
					<ReportText Type="Newline"/>
				</Branch>
				<Branch>
				    <SectionIf DataElementId="best" />
					<ReportText Type="PlainText">Best screening interval for patient: </ReportText>      
					<ReportText Type="InsertDataElementValue" Value="best"/>
					<ReportText Type="Newline"/>
				</Branch>
				
				</ReportSection>
		</ReportSections>
		</EndPoint>	
	</EndPoints>

</ReportingModule>