          <Display Value="RDE779" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE779" />
        </Coding>
      </CodableConcept>
      <Label>Previous history</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Prior surgery</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
          <Label>Implants</Label>
        </Choice>
        <Choice>
          <Value>2</Value>
          <Label>Not significant</Label>
        </Choice>
