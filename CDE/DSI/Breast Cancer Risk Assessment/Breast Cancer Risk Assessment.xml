          <Code Value="RDE780" />
          <Display Value="RDE780" />
          <UserSelected Value="false" />
          <Url Value="http://radelement.org/element/RDE780" />
        </Coding>
      </CodableConcept>
      <Label>Views</Label>
      <ChoiceInfo>
        <Choice>
          <Value>0</Value>
          <Label>Standard craniocaudal</Label>
        </Choice>
        <Choice>
          <Value>1</Value>
