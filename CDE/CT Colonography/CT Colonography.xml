                    <UserSelected Value="false" />
                    <Url Value="http://radelement.org/element/RDE75" />
                </Coding>
            </CodableConcept>
            <Label>Colonic findings assessment score</Label>
            <Hint />
            <ChoiceInfo>
                <Choice>
                    <Value>Inadequate study / awaiting prior comparison</Value>
                    <Label>Inadequate study / awaiting prior comparison</Label>
                </Choice>
                <Choice>
                    <Value>Normal colon or benign lesion</Value>
                    <Label>Normal colon or benign lesion</Label>
                </Choice>
                <Choice>
                    <Value>Intermediate polyp or indeterminate finding</Value>
                    <Label>Intermediate polyp or indeterminate finding</Label>
                </Choice>
                <Choice>
                    <Value>Polyp, possibly advanced adenoma</Value>
                    <Label>Polyp, possibly advanced adenoma</Label>
                </Choice>
                <Choice>
                    <Value>Colonic mass, likely malignant</Value>
                    <Label>Colonic mass, likely malignant</Label>
                </Choice>
            </ChoiceInfo>
        </ChoiceDataElement>
