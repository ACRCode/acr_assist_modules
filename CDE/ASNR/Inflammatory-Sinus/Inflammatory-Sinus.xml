    <EndPoints>
        <TemplatePartial Id="ExamPartial">
            <Branch>
                <ReportText Type="PlainText">EXAM TYPE: BRAIN MRI W/WO CONTRAST</ReportText>
                <ReportText Type="Newline" />
                <ReportText Type="PlainText">EXAM DATE AND TIME: 5/9/2018 1:30 PM EDT</ReportText>
                <ReportText Type="Newline" />
                <ReportText Type="PlainText">INDICATION: r/o demyelinating disease </ReportText>
                <ReportText Type="Newline" />
                <ReportText Type="PlainText">COMPARISON: NONE</ReportText>
                <ReportText Type="Newline" />
            </Branch>
        </TemplatePartial>
        <EndPoint Id="macroEndpoint">
            <Label>A</Label>
            <ReportSections>
                <ReportSection SectionId="findings">
                    <Branch>
                        <AndCondition>
                            <SectionIf DataElementId="laterallamella" />
                            <SectionIf DataElementId="lateralpositions" />
                            <SectionIf DataElementId="kerosclass" />
                        </AndCondition>
                        <ReportText Type="PlainText">The </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="laterallamella" />
                        <ReportText Type="PlainText"> lateral lamella are </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="lateralpositions" />
                        <ReportText Type="PlainText"> with Keros class </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="kerosclass" />
                        <ReportText Type="Newline" />
                    </Branch>
                    <Branch>
                        <SectionIf DataElementId="ethmoidarteries" />
                        <ReportText Type="PlainText">The anterior ethmoid arteries are </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="ethmoidarteries" />
                        <ReportText Type="Newline" />
                    </Branch>
                    <Branch>
                        <SectionIf DataElementId="maxillarysinuses" />
                        <ReportText Type="PlainText">The maxillary sinuses are </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="maxillarysinuses" />
                        <ReportText Type="Newline" />
                    </Branch>
                    <Branch>
                        <SectionIf DataElementId="uncinateprocesses" />
                        <ReportText Type="PlainText">The uncinate processes are </ReportText>
                        <ReportText Type="InsertDataElementValue" Value="uncinateprocesses" />
                        <ReportText Type="Newline" />
                    </Branch>
                    <Branch>
                        <SectionIf DataElementId="maxillaryinfundibulum" />
