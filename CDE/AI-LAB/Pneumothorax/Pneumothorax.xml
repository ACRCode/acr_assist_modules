<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>Pneumothorax</Label>
		<ID>Pneumothorax</ID>
		<SchemaVersion>1.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<Info>
			<Description>This module describes the Common Data elements and Macros for Pneumothorax</Description>
			
			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>
			
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.jpg</Location>
					<Label>Pneumothorax key image</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>C</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>
		
	</Metadata>

	
	<DataElements>
	
	   <NumericDataElement Id="Age" IsRequired="true" DisplaySequence="1">
			<Label>Age of the patient</Label>
			<Minimum>0.00</Minimum>
			<Maximum>90.00</Maximum>
		</NumericDataElement>
	
	    <ChoiceDataElement Id="Sex" IsRequired="true" DisplaySequence="2">
			<Label>Sex of the patient</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Famale</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Male</Label>
				</Choice>			
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		
		<ChoiceDataElement Id="chest_trauma" IsRequired="true" DisplaySequence="3">
			<Label>Chest trauma</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="disease" IsRequired="true" DisplaySequence="4">
			<Label>Presence of lung disease</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="Pneumomediastinum" IsRequired="true" DisplaySequence="5">
			<Label>Pneumomediastinum</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="lung_involvement" IsRequired="true" DisplaySequence="6">
			<Label>Lung tissue involvement</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Complete collapse</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Partial collapse</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="tension" IsRequired="true" DisplaySequence="7">
			<Label>Tension pneumothorax</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="skin_artifact" IsRequired="true" DisplaySequence="8">
			<Label>Skin fold artifact</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="pneumothorax_detection" IsRequired="true" DisplaySequence="9">
			<Label>Pneumothorax detection</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<NumericDataElement Id="separation" IsRequired="true" DisplaySequence="10">
			<Label>Pleural separation in mm</Label>
			<Minimum>0.00</Minimum>
			<Maximum>40.00</Maximum>
		</NumericDataElement>
		
		<ChoiceDataElement Id="side" IsRequired="true" DisplaySequence="11">
			<Label>Pneumothorax side</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Right</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Left</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Bilateral</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="size" IsRequired="true" DisplaySequence="12">
			<Label>Pneumothorax size</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>Small</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Medium</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Large</Label>
				</Choice>				
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="chest_tube" IsRequired="true" DisplaySequence="13">
			<Label>Presence of chest tube</Label>
			<ChoiceInfo>
			<Choice>
					<Value>1</Value>
					<Label>Absent</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>Present</Label>
				</Choice>
                <Choice>
					<Value>3</Value>
					<Label>Undetermined</Label>
				</Choice>						
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="scanner_manufacturer" IsRequired="true" DisplaySequence="14">
			<Label>Scanner manufacturer</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>A</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>B</Label>
				</Choice>			
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="scanner_model" IsRequired="true" DisplaySequence="15">
			<Label>Scanner model</Label>
			<ChoiceInfo>
				<Choice>
					<Value>1</Value>
					<Label>A</Label>	
				</Choice>
				<Choice>
					<Value>2</Value>
					<Label>B</Label>
				</Choice>			
			</ChoiceInfo>			
		</ChoiceDataElement>
		
		
		</DataElements>

	<Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>
		
	</Rules>

	<EndPoints>
		
	 
	 
		<EndPoint Id="macroEndpoint">
		<Label>A</Label>
		 <ReportTexts>
				<ReportText SectionId="findings">	
				</ReportText>			
		</ReportTexts>		
		</EndPoint>
	</EndPoints>

</ReportingModule>