<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>IF-Pituitary CT MRI</Label>
		<ID>ACR_IF-PituitaryCTMRI</ID>
		<SchemaVersion>2.0</SchemaVersion>
		<ModuleVersion>1.0</ModuleVersion>
		<CreatedDate>2018-06-20</CreatedDate>
    	<LastModifiedDate>2018-11-30</LastModifiedDate>
		<ApprovedBy>American College of Radiology</ApprovedBy>
		<ReviewedBy>American College of Radiology</ReviewedBy>
		<DevelopedBy>American College of Radiology</DevelopedBy>
		<RadElementSetID>MID2244</RadElementSetID>
		<Info>
			<Description>IF-Pituitary Findings for CT or MRI</Description>
			<References>
			    <Citation Url="https://nrdr.acr.org/lirads/">
                ACR LI-RADS</Citation>
                <Citation Url="https://www.acr.org/Practice-Management-Quality-Informatics/Informatics/Structured-Content">
                ACR Assist</Citation>
			</References>
			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keyimage.PNG</Location>
					<Label>IF-PituitaryCTMRI</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>IF Pituitary</ReportCitationText>
        <ApplicableSexes Value="Both"></ApplicableSexes>

	</Metadata>
	
	<DataElements>
		
		<GlobalValue Id="conditionConst">10</GlobalValue>
	
		<ChoiceDataElement Id="typeofcyst" IsRequired="true" DisplaySequence="1" CdeId="RDE65" Editable="true" Hasprefilled="false" Output="false">
			<Label>Composition of Cyst</Label>
			<CodableConcept Name="typeofcyst">
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>
    				<UserSelected Value="true"></UserSelected>
    			</Coding>
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>    				
    			</Coding>
    		</CodableConcept>
			<Hint> If the composition of the lesion cannot be determined, it should be managed as if solid</Hint>
			<ChoiceInfo>
				<Choice>
					<Value>simplecyst</Value>
					<Label>Simple Cyst</Label>	
				</Choice>
				<Choice>
					<Value>mixed</Value>
					<Label>Mixed-Solid Cystitc</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="mixedcystsize" IsRequired="true" DisplaySequence="2" CdeId="RDE65" Editable="true" Hasprefilled="false" Output="false">
			<Label>Size of Mixed-Solid Cyst</Label>
			<CodableConcept Name="mixedcystsize">
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>
    				<UserSelected Value="true"></UserSelected>
    			</Coding>
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>    				
    			</Coding>
    		</CodableConcept>	
			<Hint>COnsider the possibility of a pseudolesion given the lesion's size. Pseudolesions require no further management.</Hint>
			<ChoiceInfo>
				<Choice>
					<Value>lessthan5mm</Value>
					<Label>Less than 5mm</Label>	
				</Choice>
				<Choice>
					<Value>between5and10mm</Value>
					<Label>5-10mm</Label>	
				</Choice>
				<Choice>
					<Value>greaterthan10mm</Value>
					<Label>Greater than 10mm</Label>	
				</Choice>
			</ChoiceInfo>
			<ConditionalProperties>
				<ConditionalProperty>					
					<EqualCondition DataElementId="abletoevaluatecomposition" ComparisonValue="no"/> 
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
				<ConditionalProperty>					
					<EqualCondition DataElementId="typeofcyst" ComparisonValue="simplecyst"/> 
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
			</ConditionalProperties>	
		</ChoiceDataElement>	
		
		<ChoiceDataElement Id="masseffect" IsRequired="true" DisplaySequence="3" CdeId="RDE65" Editable="true" Hasprefilled="false" Output="false">
			<Label>Has Mass Effect?</Label>
			<CodableConcept Name="masseffect">
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>
    				<UserSelected Value="true"></UserSelected>
    			</Coding>
    			<Coding>
    				<System Value="Test"></System>
    				<Version Value="1.0"></Version>
    				<Code Value="RDETest"></Code>
    				<Display Value="Test"></Display>    				
    			</Coding>
    		</CodableConcept>	
			<ChoiceInfo>
				<Choice>
					<Value>yes</Value>
					<Label>Yes</Label>	
				</Choice>
				<Choice>
					<Value>no</Value>
					<Label>No</Label>	
				</Choice>
			</ChoiceInfo>
			<ConditionalProperties>
				<ConditionalProperty>
					<EqualCondition DataElementId="abletoevaluatecomposition" ComparisonValue="no"/> 
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
				<ConditionalProperty>
				<OrCondition>
					<EqualCondition DataElementId="mixedcystsize" ComparisonValue="between5and10mm"/> 
					<EqualCondition DataElementId="mixedcystsize" ComparisonValue="lessthan5mm"/> 					
				</OrCondition>	
				<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
			</ConditionalProperties>
		</ChoiceDataElement>
		
		<ChoiceDataElement Id="invasion" IsRequired="true" DisplaySequence="4" CdeId="RDE65" Editable="true" Hasprefilled="false" Output="false">
			<Label>Has Invasion into surrounding structures?</Label>
			<Hint>Invasion into surrounding Structures include Optic Chiasm, Optic Nerve and Cavernous Sinus</Hint>
			<ChoiceInfo>
				<Choice>
					<Value>yes</Value>
					<Label>Yes</Label>	
				</Choice>
				<Choice>
					<Value>no</Value>
					<Label>No</Label>	
				</Choice>
			</ChoiceInfo>
			<ConditionalProperties>
				<ConditionalProperty>
					<EqualCondition DataElementId="abletoevaluatecomposition" ComparisonValue="no"/> 
					<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
				<ConditionalProperty>
				<OrCondition>
					<EqualCondition DataElementId="mixedcystsize" ComparisonValue="between5and10mm"/> 
					<EqualCondition DataElementId="mixedcystsize" ComparisonValue="lessthan5mm"/> 					
				</OrCondition>	
				<IsRelevant>false</IsRelevant>
				</ConditionalProperty>
				<!-- <ConditionalProperty>
					<EqualCondition DataElementId="typeofcyst" ComparisonValue="mixed"/> 
					<IsReleveant>false</IsReleveant>
				</ConditionalProperty -->
			</ConditionalProperties>
		</ChoiceDataElement>	
		
	</DataElements>	
	
	<Rules>
	
	<DecisionPoint Id="incidentalpituitary">
      <Label>IncidentalCTMRI</Label>
      <Branch>
		<Label>Simple</Label>

		<EqualCondition DataElementId="typeofcyst" ComparisonValue="simplecyst"/>
		
		<DecisionPoint Id="Simple">
		 <Label>Simple nomass</Label>
		   <Branch>
		    <Label>Simple no mass</Label>
				<EqualCondition DataElementId="masseffect" ComparisonValue="no"/>
			<EndPointRef EndPointId="NoFurtherEvaluation"></EndPointRef>
	        </Branch>

     		  <Branch>
				<Label>Simple cyst yes mass</Label>
					<EqualCondition DataElementId="masseffect" ComparisonValue="yes"/>
					<EndPointRef EndPointId="referral"></EndPointRef>
			  </Branch>
	      </DecisionPoint>
		</Branch>
		  
		  
	  <Branch>
		<Label>Mixed yes mass</Label>

		   <AndCondition>
			<EqualCondition DataElementId="typeofcyst" ComparisonValue="mixed"/>
			<EqualCondition DataElementId="mixedcystsize" ComparisonValue="lessthan5mm"/>
           </AndCondition>
			<EndPointRef EndPointId="Nofurtherimgaing"></EndPointRef>
	  </Branch>
	  
	  <Branch>
		<Label>Mixed no mass</Label>
		   <AndCondition>
			<EqualCondition DataElementId="typeofcyst" ComparisonValue="mixed"/>
			<EqualCondition DataElementId="mixedcystsize" ComparisonValue="between5and10mm"/>
           </AndCondition>
			<EndPointRef EndPointId="Nofurtherimgaingabove5"></EndPointRef>
	  </Branch>
	  
	  <Branch>
		<Label>Mixed with mass</Label>
		   <AndCondition>
			<EqualCondition DataElementId="typeofcyst" ComparisonValue="mixed"/>
			<EqualCondition DataElementId="mixedcystsize" ComparisonValue="greaterthan10mm"/>
			<EqualCondition DataElementId="masseffect" ComparisonValue="yes"/>
           </AndCondition>
			<EndPointRef EndPointId="referral"></EndPointRef>
	  </Branch>
	  
	  
	  <Branch>
		<Label>Mixed with no mass</Label>
		   <AndCondition>
			<EqualCondition DataElementId="typeofcyst" ComparisonValue="mixed"/>
			<EqualCondition DataElementId="mixedcystsize" ComparisonValue="greaterthan10mm"/>
			<EqualCondition DataElementId="masseffect" ComparisonValue="no"/>
           </AndCondition>
			<EndPointRef EndPointId="recommend"></EndPointRef>
	  </Branch>
	 
	  
	</DecisionPoint>
		
	
	
	</Rules>
	
	
	<EndPoints>
		
		<EndPoint Id="NoFurtherEvaluation">
		<Label>NoFurtherEvaluation</Label>
		  <ReportTexts>
			<ReportText SectionId="findings">No Further Evaluation</ReportText>
		  </ReportTexts>
		</EndPoint>
		
		<EndPoint Id="referral">
		<Label>Referral</Label>
		  <ReportTexts>
			<ReportText SectionId="findings">Referral for endocrine and/or neurosurgical management</ReportText>
		  </ReportTexts>
		</EndPoint>
		
		<EndPoint Id="Nofurtherimgaing">
		<Label>Nofurtherimgaing</Label>
		  <ReportTexts>
			<ReportText SectionId="findings">No further imaging &amp;&nbsp; Correlate with history of pituitary hypersecretion.</ReportText>
		  </ReportTexts>
		</EndPoint>
		
	   <EndPoint Id="Nofurtherimgaingabove5">
		<Label>Nofurtherimgaingabove5</Label>
		  <ReportTexts>
			<ReportText SectionId="findings">No further imaging &amp;&nbsp; Correlate with history of pituitary hypersecretion and consider endocrine function tests.</ReportText>
		  </ReportTexts>
		</EndPoint>
		
	   <EndPoint Id="recommend">
		<Label>recommend</Label>
		  <ReportTexts>
			<ReportText SectionId="findings">Recommend endocrine function tests &amp;&nbsp; 6-12 month follow-up pituitary MRI.</ReportText>
		  </ReportTexts>
		</EndPoint>
		
	</EndPoints>
	
	</ReportingModule>
