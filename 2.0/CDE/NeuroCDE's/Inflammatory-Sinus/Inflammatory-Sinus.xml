<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../../XML Schema/ACRAssist_xml_schema.rnc" type="application/relax-ng-compact-syntax"?>
<ReportingModule>
	<Metadata>
		<Label>Inflammatory Sinus</Label>
		<ID>Inflammatory_Sinus</ID>
		<SchemaVersion>2.0</SchemaVersion>
        <ModuleVersion>2.0</ModuleVersion>
        <CreatedDate>2018-12-31</CreatedDate>
        <LastModifiedDate>2018-12-06</LastModifiedDate>
		<ApprovedBy>American College of Radiology</ApprovedBy>
		<ReviewedBy>American College of Radiology</ReviewedBy>
		<DevelopedBy>American College of Radiology</DevelopedBy>
		<Info>
			<Description>This module describes the Common Data elements and Macros for Inflammatory Sinus</Description>

			<References>
				<Citation Url="http://radelement.org/"> CDE URL </Citation> 
			</References>

			<Diagrams>
				<Diagram DisplaySequence="1" IsKeyDiagram="true">
					<Location>keydiagram.png</Location>
					<Label>Inflammatory Sinus key image</Label>
				</Diagram>
				<Diagram DisplaySequence="2" >
					<Location>algorithm for morphologic classification.png</Location>
					<Label>Algorithm for morphologic classification</Label>
				</Diagram>
				<Diagram DisplaySequence="3" >
					<Location>supporting.jpg</Location>
					<Label>Supporting</Label>
				</Diagram>
			</Diagrams>
			<Contact>
				<Name>ACR Assist</Name>
				<Email>acr-assist@acr.org</Email>
				<Institution>American College of Radiology</Institution>
			</Contact>
		</Info>
		<ReportCitationText>Inflammatory Sinus</ReportCitationText>
		<ApplicableSexes Value="Both"></ApplicableSexes>

	</Metadata>

	<DataElements>

		<GlobalValue Id="conditionConst">10</GlobalValue>
		<ChoiceDataElement Id="laterallamella" DisplaySequence="1" IsRequired="true" >
			<Label>Lateral Lamella</Label>
			<ChoiceInfo>
				<Choice>
					<Value>left</Value>
					<Label>Left</Label>
				</Choice>
				<Choice>
					<Value>right</Value>
					<Label>Right</Label>
				</Choice>

				<Choice>
					<Value>bilateral</Value>
					<Label>Bilateral</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>

		<ChoiceDataElement Id="lateralpositions" DisplaySequence="2" IsRequired="true">
			<Label>Lateral Positions</Label>
			<ChoiceInfo>
				<Choice>
					<Value>normal</Value>
					<Label>Normal in position</Label>
				</Choice>
				<Choice>
					<Value>elongated</Value>
					<Label>Elongated</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>

		<ChoiceDataElement Id="kerosclass" DisplaySequence="3" IsRequired="true">
			<Label>Keros Class</Label>
			<ChoiceInfo>
				<Choice>
					<Value>none</Value>
					<Label>None</Label>
				</Choice>
				<Choice>
					<Value>I</Value>
					<Label>I</Label>
				</Choice>
				<Choice>
					<Value>II</Value>
					<Label>II</Label>
				</Choice>
				<Choice>
					<Value>III</Value>
					<Label>III</Label>
				</Choice>
				<Choice>
					<Value>IV</Value>
					<Label>IV</Label>
				</Choice>
				<Choice>
					<Value>V</Value>
					<Label>V</Label>
				</Choice>
				<Choice>
					<Value>VI</Value>
					<Label>VI</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>

		<ChoiceDataElement Id="ethmoidarteries" DisplaySequence="4" IsRequired="true">
			<Label>Ethmoid arteries</Label>
			<ChoiceInfo>
				<Choice>
					<Value>normal</Value>
					<Label>Normal</Label>
				</Choice>
				<Choice>
					<Value>exposed</Value>
					<Label>Exposed</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>

		<IntegerDataElement Id="maxillarysinuses" IsRequired="true" DisplaySequence="5">
			<Label>Maxillary sinuses</Label>
			<Minimum>1</Minimum>
			<Maximum>1000</Maximum>
		</IntegerDataElement>	

		<ChoiceDataElement Id="uncinateprocesses" DisplaySequence="6" IsRequired="true">
			<Label>Uncinate Processes</Label>
			<ChoiceInfo>
				<Choice>
					<Value>normal</Value>
					<Label>Normal</Label>
				</Choice>
				<Choice>
					<Value>lateralized</Value>
					<Label>Lateralized</Label>
				</Choice>
			</ChoiceInfo>
		</ChoiceDataElement>	

		<IntegerDataElement Id="maxillaryinfundibulum" IsRequired="true" DisplaySequence="7">
			<Label>Maxillary Infundibulum</Label>
			<Minimum>1</Minimum>
			<Maximum>1000</Maximum>
		</IntegerDataElement>	
		<IntegerDataElement Id="osteomeatalunits" IsRequired="true" DisplaySequence="8">
			<Label>Osteomeatalunits</Label>
			<Minimum>1</Minimum>
			<Maximum>1000</Maximum>
		</IntegerDataElement>	

		<IntegerDataElement Id="sphenoethmoidalrecesses" IsRequired="true" DisplaySequence="9">
			<Label>Sphenoethmoidal recesses</Label>
			<Minimum>1</Minimum>
			<Maximum>1000</Maximum>
		</IntegerDataElement>


		<IntegerDataElement Id="sphenoidsinuses" IsRequired="true" DisplaySequence="10">
			<Label>Sphenoid sinuses</Label>
			<Minimum>1</Minimum>
			<Maximum>1000</Maximum>
		</IntegerDataElement>	

	</DataElements>

	<Rules>
		<DecisionPoint Id="macrodp">
			<Label>Macro Banch</Label>
			<Branch>
				<EqualCondition DataElementId="conditionConst" ComparisonValue="10"/>
				<EndPointRef EndPointId="macroEndpoint"></EndPointRef>
			</Branch>
		</DecisionPoint>

	</Rules>

	<EndPoints>
		<TemplatePartial Id="ExamPartial">
			<Branch>
				<ReportText Type="PlainText">EXAM TYPE: BRAIN MRI W/WO CONTRAST</ReportText>
				<ReportText Type="Newline"></ReportText>
				<ReportText Type="PlainText">EXAM DATE AND TIME: 5/9/2018 1:30 PM EDT</ReportText>
				<ReportText Type="Newline"></ReportText>
				<ReportText Type="PlainText">INDICATION: r/o demyelinating disease </ReportText>
				<ReportText Type="Newline"></ReportText>
				<ReportText Type="PlainText">COMPARISON: NONE</ReportText>
				<ReportText Type="Newline"></ReportText>
			</Branch>
		</TemplatePartial>

		<EndPoint Id="macroEndpoint">
			<Label>A</Label>
			<ReportSections>    
				<ReportSection SectionId="findings">
					<Branch>
						<AndCondition>
							<SectionIf DataElementId="laterallamella"/>
							<SectionIf DataElementId="lateralpositions"/>
							<SectionIf DataElementId="kerosclass"/>
						</AndCondition>
						
						<ReportText Type="PlainText">The </ReportText>
						<ReportText Type="InsertDataElementValue" Value="laterallamella"></ReportText>
						<ReportText Type="PlainText"> lateral lamella are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="lateralpositions"></ReportText>
						<ReportText Type="PlainText"> with Keros class </ReportText>
						<ReportText Type="InsertDataElementValue" Value="kerosclass"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="ethmoidarteries"/>
						<ReportText Type="PlainText">The anterior ethmoid arteries are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="ethmoidarteries"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="maxillarysinuses"/>
						<ReportText Type="PlainText">The maxillary sinuses are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="maxillarysinuses"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="uncinateprocesses"/>
						<ReportText Type="PlainText">The uncinate processes are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="uncinateprocesses"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="maxillaryinfundibulum"/>
						<ReportText Type="PlainText">The maxillary infundibulum are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="maxillaryinfundibulum"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="osteomeatalunits"/>
						<ReportText Type="PlainText">The osteomeatal units are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="osteomeatalunits"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="sphenoethmoidalrecesses"/>
						<ReportText Type="PlainText">The sphenoethmoidal recesses are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="sphenoethmoidalrecesses"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
					
					<Branch>
						<SectionIf DataElementId="sphenoidsinuses"/>
						<ReportText Type="PlainText">The sphenoid sinuses are </ReportText>
						<ReportText Type="InsertDataElementValue" Value="sphenoidsinuses"></ReportText>
						<ReportText Type="Newline"></ReportText>
					</Branch>
				</ReportSection>
			</ReportSections>
		</EndPoint>
	</EndPoints>
</ReportingModule>
